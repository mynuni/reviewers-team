<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>

		<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="reviewLike" type="com.spring.biz.reviewLike.ReviewLikeVO"/>
		<typeAlias type="com.spring.biz.UserBlackList.UserBlackListVO" alias="blackLIst"/>
		<typeAlias type="com.spring.biz.reportReview.ReportReviewVO" alias="reportReview"/>
		<typeAlias type="com.spring.biz.tableNum.TableNumVO" alias="tableValue"/>
		<typeAlias type="com.spring.biz.reportComment.ReportCommentVO" alias="reportComment"/>
		<typeAlias type="com.spring.biz.user.UserVO" alias="user"/>
		<typeAlias type="com.spring.biz.userInfo.UserInfoVO" alias="userInfo"/>
		<typeAlias alias="board" type="com.spring.biz.board.ReviewBoardVO"></typeAlias>
		<typeAlias alias="cnt"	type="com.spring.biz.CntHistory.CntHistoryVO"></typeAlias>
		<typeAlias type="com.spring.biz.hashTag.HashTagVO" alias="hashTag"/>
		<typeAlias type="com.spring.biz.MovieGenres.MovieGenresVO" alias="MovieGeners"/>
		<typeAlias alias="FaqBoardVO" type="com.spring.biz.support.faq.FaqBoardVO"/>
		<typeAlias alias="QnaBoardVO" type="com.spring.biz.support.qna.QnaBoardVO"/>
		<typeAlias alias="QnaCommentVO" type="com.spring.biz.support.qnaCmt.QnaCommentVO"/>
		<typeAlias alias="FileUploadVO" type="com.spring.biz.support.qna.FileUploadVO"/>
		<typeAlias alias="MyContentsVO" type="com.spring.biz.mypage.MyContentsVO"/>
		<typeAlias alias="MyStarVO" type="com.spring.biz.mypage.MyStarVO"/>
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/reviewLike-mapping.xml"/>
		<mapper resource="mappings/user-mapping.xml"/>
		<mapper resource="mappings/userInfo-mapping.xml"/>
		<mapper resource="mappings/board-mapping.xml"/>
		<mapper resource="mappings/HashTag-mapping.xml"/>
		<mapper resource="mappings/BlackList-mapping.xml"/>
		<mapper resource="mappings/MovieGeners-mapping.xml"/>
		<mapper resource="mappings/tableNum-mapping.xml"/>
		<mapper resource="mappings/ReportReview-mapping.xml"/>
		<mapper resource="mappings/ReportComment-mapping.xml"/>
		<mapper resource="mappings/reply-mapping.xml"/>
		<mapper resource="mappings/faq-mapping.xml"/>
		<mapper resource="mappings/qna-mapping.xml"/>
		<mapper resource="mappings/qnacmt-mapping.xml"/>
		<mapper resource="mappings/mypage-mapping.xml"/>
	</mappers>
</configuration>